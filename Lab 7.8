#include <stdio.h>

void reverseArray(char* arr[], int n)

{

    char* temp;

    int j = n - 1;

    for (int i = 0; i < j; i++) {

        temp = arr[i];

        arr[i] = arr[j];

        arr[j] = temp;

        j--;

    }

}

  

